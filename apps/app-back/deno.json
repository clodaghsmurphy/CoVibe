{
    "name": "app-back",
  "tasks": {
    "dev": "deno run  --allow-env  --allow-net  --allow-read --allow-write --watch src/main.ts",
    "test": "deno test --watch --unstable --allow-read --allow-write --allow-net --allow-env --allow-run --allow-plugin --allow-hrtime --allow-all --no-check main.ts",
    "build": "deno compile --check --allow-read --allow-env --allow-net --output dist/main src/main.ts"
  },
  "imports": {
    "@nestjs/common": "npm:@nestjs/common@^10.4.6",
    "@nestjs/config": "npm:@nestjs/config@^3.3.0",
    "@nestjs/core": "npm:@nestjs/core@^10.4.6",
    "@nestjs/platform-express": "npm:@nestjs/platform-express@^10.4.6",
    "@nestjs/swagger": "npm:@nestjs/swagger@^7.4.2",
    "@nestjs/terminus": "npm:@nestjs/terminus@^10.2.3",
    "@std/assert": "jsr:@std/assert@1",
    "@std/testing": "jsr:@std/testing@^1.0.5",
    "prisma": "npm:prisma@^5.22.0"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
